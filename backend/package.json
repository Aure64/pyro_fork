    "@types/node": "14.14.41",
    "@types/node-notifier": "8.0.0",
    "@types/node-telegram-bot-api": "0.51.1",
    "@types/nodemailer": "6.4.1",
    "@types/ramda": "0.27.40",
    "@types/validatorjs": "3.15.0",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">= 14.15.3"
  },
  "publishConfig": {
    "@tezos-kiln:registry": "https://gitlab.com/api/v4/projects/22897259/packages/npm/"
  },
  "dependencies": {
    "@slack/webhook": "6.0.0",
