{
  "name": "@tezos-kiln/backend",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/server.js",
  "scripts": {
    "build": "yarn -s clean && tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/server.ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "eslint:lint": "eslint . --ext .ts",
    "lint": "yarn eslint:lint && yarn tsc:lint && yarn prettier:lint",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "prettier:lint": "prettier --check 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc:lint": "tsc --noEmit -p ."
  },
  "devDependencies": {
    "@types/node": "14.14.14",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "prettier": "2.2.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">= 15.4.0"
  },
  "publishConfig": {
    "@tezos-kiln:registry":"https://gitlab.com/api/v4/projects/22897259/packages/npm/"
  }
}
