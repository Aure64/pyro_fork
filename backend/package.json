    "@types/node-notifier": "8.0.0",
    "@types/nodemailer": "6.4.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">= 14.15.3"
  },
  "publishConfig": {
    "@tezos-kiln:registry": "https://gitlab.com/api/v4/projects/22897259/packages/npm/"
