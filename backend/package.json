    "nconf": "0.11.2",
    "node-notifier": "9.0.1",
    "node-telegram-bot-api": "0.52.0",
    "nodemailer": "6.5.0",
    "ramda": "0.27.1",
    "runtypes": "6.0.0",
    "sqlite3": "5.0.2",
    "validatorjs": "3.22.1",
    "yargs": "16.2.0"
  },
