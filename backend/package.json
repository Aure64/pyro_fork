    "better-queue": "3.8.10",
    "better-queue-sqlite": "git://github.com/diamondio/better-queue-sqlite#225e0f0",
    "loglevel": "1.7.1",
    "node-gyp": "7.1.2",
    "node-notifier": "9.0.0",
    "nodemailer": "6.4.17"
  }
