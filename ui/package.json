  "scripts": {
    "dev": "vite",
    "prebuild": "yarn generate:gql",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn eslint:lint && yarn tsc:lint && yarn prettier:lint",
    "prettier:lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:lint": "eslint . --ext .ts",
    "tsc:lint": "tsc --noEmit -p .",
    "generate:gql": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^6",
    "graphql": "^16.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@testing-library/react": "^12.1.5",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "@web/test-runner": "^0.13.27",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "chai": "^4.3.6",
    "prettier": "^2.6.2",
    "typescript": "^4.4.4",
    "@vitejs/plugin-react": "^1.3.0",
    "vite": "^2.9.9"
